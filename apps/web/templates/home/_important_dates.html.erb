<div class='header'>Important Dates</div>
<% if entry_list.all.any? %>
  <ul id="entry-list">
    <% entry_list.sorted(year.year, current_user).each do |entry| %>
      <% if entry.user_id %>
        <li class="private-entry">
        <% else %>
          <li class="public-entry">
          <% end %>
          <%= 
      link_to('dates/delete/' + entry.id.to_s) do
        image('remove.png')
      end
      %>
          <%= entry.display year.year %>
        </li>
      <% end %>
    </ul>
  <% else %>
    <div>There are no calendar entries stored.</div>
  <% end %>
  <div class="controls">
    <%= link_to('Add a date', routes.add_date_path(date_type: {date_type: 'Fixed'}, calendar_entry_fixed: {entry_access: 'Public'} ), class: 'btn') %>
  </div>
