<html>
<head>
  <%= stylesheet 'styles' %>
</head>
<body>
  <div class="year">
    <div class="header"><%= year.year %></div>
    <div class="container">
      <% year.months.each do |month| %> 
        <%= render partial: 'home/month', locals: { month: month } %>
      <% end %>
    </div>
  </div>
  <%# render partial: '_important_dates.erb' %>
</body>
</html>